{
  "name": "mongoback",
  "version": "0.0.7",
  "description": "An npm module to easily export your MongoDB",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "transpile": "tsc",
    "db:clean": "mongo-cleaner clean -y",
    "db:restore": "mongorestore test/mock --quiet",
    "db:populate": "npm run db:clean && npm run db:restore",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "test": "mocha --reporter spec",
    "commit": "git-cz"
  },
  "author": "Eugenio Vinicio Berretta <euberdeveloper@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euberdeveloper/mongoback.git"
  },
  "bugs": {
    "url": "https://github.com/euberdeveloper/mongoback/issues"
  },
  "homepage": "https://github.com/euberdeveloper/mongoback",
  "dependencies": {
    "chalk": "^3.0.0",
    "command-exists": "^1.2.8",
    "mongo-scanner": "^1.4.4",
    "ora": "^4.0.3",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/mongodb": "^3.3.14",
    "@types/shelljs": "^0.8.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "dree": "^2.4.6",
    "mocha": "^6.2.2",
    "mongo-cleaner": "^3.2.0",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
