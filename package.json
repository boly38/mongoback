{
  "name": "mongoexporter",
  "version": "1.0.0",
  "description": "An npm command line program to easily extract data from mongodb",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "tsc",
    "start": "node dist/index",
    "serve": "npm run transpile && npm run start",
    "db:clean": "node test/cleanDB",
    "db:restore": "mongorestore test/mock",
    "db:populate": "npm run db:clean && npm run db:restore",
    "test": "mocha --reporter spec",
    "commit": "git-cz"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "command-exists": "^1.2.8",
    "inquirer": "^7.0.0",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "is-valid-path": "^0.1.1",
    "mongodb": "^3.3.4",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/is-valid-path": "^0.1.0",
    "@types/mongodb": "^3.3.11",
    "@types/shelljs": "^0.8.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "dree": "^2.4.6",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
